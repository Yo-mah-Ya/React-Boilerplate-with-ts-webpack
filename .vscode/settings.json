{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true,
    "npm-packages-offline-cache": true,
    "dist": true,
    "coverage": true
  },
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "eslint.packageManager": "yarn",
  "eslint.validate": ["javascript", "typescript"],
  "eslint.lintTask.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "npm.packageManager": "yarn",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "[css]": {
    "editor.formatOnSave": false
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "cSpell.enabled": true,
  "cSpell.words": [
    "addmissingownerfields",
    "addtimestampfields",
    "avif",
    "awscloudformation",
    "birthdate",
    "breakcirculardependency",
    "browserslist",
    "browserconfig",
    "cleangeneratedmodelsdirectory",
    "codegen",
    "emitauthprovider",
    "enableiterativegsiupdates",
    "enablexcodeintegration",
    "enablecaseinsensitivity",
    "enabledartnullsafety",
    "flexbox",
    "flexbugs",
    "forcealiasattributes",
    "formdata",
    "graphqltransformer",
    "generateindexrules",
    "generategraphqlpermissions",
    "handlelistnullabilitytransparently",
    "icss",
    "improvepluralization",
    "Keepalive",
    "msapplication",
    "nocheck",
    "openapi",
    "opensearch",
    "opensearchdescription",
    "pathinfo",
    "preconnect",
    "prefetch",
    "retaincasestyle",
    "secondarykeyasgsi",
    "skipoverridemutationinputtypes",
    "suppressschemamigrationprompt",
    "securityenhancementnotification",
    "showfieldauthnotification",
    "stylelint",
    "stylelintrc",
    "tailwindcss",
    "transformerversion",
    "tsbuildinfo",
    "Unauth",
    "unauth",
    "usesubusernamefordefaultidentityclaim",
    "useinclusiveterminology",
    "useenabledmfas",
    "useappsyncmodelgenplugin",
    "usedocsgeneratorplugin",
    "usetypesgeneratorplugin",
    "useexperimentalpipelinedtransformer",
    "latestregionsupport",
    "validatetypenamereservedwords",
    "zoneinfo"
  ],
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  }
}
